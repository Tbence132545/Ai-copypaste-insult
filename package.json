{
  "name": "ai-detector",
  "displayName": "AI Detector",
  "description": "Detects AI-generated code in VS Code and playfully insults the user if AI code is detected.",
  "version": "0.0.1",
  "publisher": "bence132",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tbence132545/Ai-copypaste-insult.git"
  },
  "keywords": [
    "ai",
    "detector",
    "copypaste",
    "code",
    "fun"
  ],
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "ai-detector.checkCode",
        "title": "Check code with AI Detector"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
